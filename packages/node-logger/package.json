{
  "name": "node-logger",
  "version": "0.0.6",
  "license": "ISC",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p  .",
    "dev": "tsc -w .",
    "test": "node --inspect node_modules/.bin/jest --watch",
    "pretest": "npm run build",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@sentry/node": "^5.10.2",
    "winston": "^3.2.1"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --ext .js,.jsx,.ts,.tsx",
      "git add"
    ]
  }
}
